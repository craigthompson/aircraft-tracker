{
  "name": "aircraft-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:backend": "nodemon server/index.js -w server -w src -w database -w config -w utils",
    "dev": "vite",
    "build": "vite build",
    "db:create": "createdb aircraft_tracker && npm run db:sync",
    "db:sync": "node database/model.js",
    "db:seed": "node database/seeding/seed.js",
    "db:drop": "chmod +x confirmDbDrop.sh && ./confirmDbDrop.sh",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "leaflet": "^1.9.4",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.3",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-leaflet": "^4.2.1",
    "react-leaflet-custom-control": "^1.4.0",
    "sequelize": "^6.37.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "vite-express": "^0.16.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.0"
  }
}
